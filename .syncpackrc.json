{
  "dependencyTypes": ["dev", "prod"],
  "semverGroups": [
    {
      "label": "use caret ranges",
      "packages": ["**"],
      "dependencyTypes": ["dev", "prod"],
      "dependencies": ["**"],
      "range": "^"
    }
  ],
  "versionGroups": [
    {
      "label": "Ignore dependencies, which have different versions between packages",
      "packages": ["**"],
      "dependencyTypes": ["dev", "prod"],
      "dependencies": [
        "jest", "ts-jest", "prettier", "eslint-plugin-markdown",
        "eslint-plugin-jest", "eslint-plugin-prettier"
      ],
      "isIgnored": true
    }
  ]
}
